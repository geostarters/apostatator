{
    "extends": [
      "@geostarters"
    ],
    "globals": {
      "performance": true,
      "document": true,
      "window": true,
      "navigator": true,
      "fetch": true,
      "it": true
    },    
    "overrides": [
      {
        "files": [
        "**/*.test.js"
        ],
        "env": {
          "jest": true
        },
        "plugins": ["jest"],
        "rules": {
        "jest/no-disabled-tests": "warn",
        "jest/no-focused-tests": "error",
        "jest/no-identical-title": "error",
        "jest/prefer-to-have-length": "warn",
        "jest/valid-expect": "error"
        }
      }
    ]
}
  